{
  "version": 2,
  "name": "plant-care",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/manifest.json",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "src": "/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],

  "github": {
    "repository": "gclark0041/PlantCare"
  }
} 